<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.apps.nosacikal.nosaapps">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyBbKNHOrXnfxF6Y8aLVJoa1rls6VWOzVnU"/>

        <activity
            android:name=".Splashscreen"
            android:theme="@style/AppTheme.SplashNoActionBar"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Walkthrough"
            android:noHistory="true"/>
        <activity android:name=".view.BottomNavigation"
            android:theme="@style/AppTheme.BottomNavNoActionBar"></activity>
    </application>

    <uses-feature android:glEsVersion="0x00020000"
        android:required="true"/>

</manifest>